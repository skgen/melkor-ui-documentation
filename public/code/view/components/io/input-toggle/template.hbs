{{#if hasSlots}}
<mk-input-toggle{{{props}}}>
  {{#if slots.checkedIcon}}
  <template #checked-icon>
    <mk-icon icon="check" />
  </template>
  {{/if}}
  {{#if slots.uncheckedIcon}}
  <template #unchecked-icon>
    <mk-icon icon="close" />
  </template>
  {{/if}}
</mk-input-toggle>
{{else}}
<mk-input-toggle{{{props}}}/>
{{/if}}