{{#if hasSlots}}
<mk-input-password{{{props}}}>
  {{#if slots.leadingIcon}}
  <template #leading-icon>
    <mk-icon icon="group" />
  </template>
  {{/if}}
  {{#if slots.trailingIcon}}
  <template #trailing-icon>
    <mk-icon icon="verified_user" />
  </template>
  {{/if}}
  {{#if slots.cancel}}
  <template #cancel>
    <mk-icon icon="delete_forever" />
  </template>
  {{/if}}
</mk-input-password>
{{else}}
<mk-input-password{{{props}}}/>
{{/if}}