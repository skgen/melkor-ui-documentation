{{#if hasSlots}}
<mk-input-text{{{props}}}>
  {{#if slots.leadingIcon}}
  <template #leading-icon>
    <mk-icon icon="barcode_scanner" />
  </template>
  {{/if}}
  {{#if slots.trailingIcon}}
  <template #trailing-icon>
    <mk-icon icon="euro" />
  </template>
  {{/if}}
</mk-input-text>
{{else}}
<mk-input-text{{{props}}}/>
{{/if}}